#include<string.h>
#include<stdio.h>
int main()
{ 
char str[50];
printf("enter the string 1:");
gets(str);
char c;
int i,j,len=0;
int k,h;
for(i=0;str[i]!='\0';i++)
len++;
for(i=0;i<len-1;i++)
{
 for(j=i+1;j<len;j++)
  {
     if(str[i]>str[j])
    {
     
       c=str[i];
       str[i]=str[j];
       str[j]=c;
    }
   }
}
puts(str);
char str1[50];
printf("enter the string 2: ");
gets(str1);
len=0;
for(i=0;str1[i]!='\0';i++)
len++;
for(i=0;i<len-1;i++)
{
 for(j=i+1;j<len;j++)
  {
     if(str1[i]>str1[j])
    {
     
       c=str1[i];
       str1[i]=str1[j];
       str1[j]=c;
    }
   }
}
puts(str1);
int count=0;
for(i=0;i<len;i++)
{
if(str[i]==str1[i])
count++;
}
if(count==len)
printf ("ANAGRAM");
else
printf("NOT ANAGRAM");
}
